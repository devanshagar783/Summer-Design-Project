<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sdp.remotehealthcareapp">

    <application
        android:fullBackupContent="true"
        android:icon="@mipmap/healtcare_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/healtcare_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.PhoneAuthActivity"></activity>
        <activity
            android:name=".Activities.Splash"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.JoyInterface"
            android:allowBackup="true"
            android:icon="@mipmap/healtcare_icon"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/healtcare_icon_round"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".Activities.Intro_Signin"
            android:theme="@style/AppTheme" />
        <activity android:name=".Activities.Signin" />
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>